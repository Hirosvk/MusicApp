<h1>Track Info</h1>

<table>
  <tr>
    <td>title</td>
    <td><%= @track.title %></td>
  </tr>

  <tr>
    <td>album</td>
    <td><%= @track.album.title %></td>
  </tr>

  <tr>
    <td>track_number</td>
    <td><%= @track.track_number %></td>
  </tr>

  <tr>
    <td>track_type</td>
    <td><%= @track.track_type %></td>
  </tr>

  <tr>
    <td>lyrics</td>
    <td><%= @track.lyrics %></td>
  </tr>

</table>

<hr>
<%= render "notes/index", notes: @track.notes%>
<%= render "notes/new", track: @track %>
